    "date-fns": "^2.29.1",
    "env-cmd": "^10.1.0",
    "formik": "^2.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-ga": "^3.3.1",
    "react-helmet": "^6.1.0",
    "react-hot-toast": "^2.3.0",
    "react-number-format": "^4.9.3",
