  "scripts": {
    "start": "react-scripts start",
    "start:local": "env-cmd -f .env.local npm run start",
    "start:prod": "env-cmd -f .env.production npm run start",
    "start:dev": "env-cmd -f .env.development npm run start",
    "build": "react-scripts build",
    "build:dev": "env-cmd -f .env.development npm run build",
    "build:prod": "env-cmd -f .env.production npm run build",
    "build:local": "env-cmd -f .env.local npm run build",
    "deploy:dev": "aws s3 sync build/ s3://dev-q.nxtcre.com ",
    "deploy:prod": "aws s3 sync build/ s3://q.nxtcre.com && aws cloudfront create-invalidation --distribution-id ESY7DNOH1209M --paths '/*'",
    "test": "npx cypress run",
    "test:spec": " npx cypress run --spec cypress/e2e/$npm_config_spec.cy.js",
    "test:open": "npx cypress open",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cypress": "^12.14.0",
    "cypress-react-selector": "^3.0.0"
  }
}
